n    load(){\n      request.get(\"/user\",{\n        params:{\n          pageNum:this.pageNum,\n          pageSize:this.pageSize,\n          search:this.search,\n        }\n      }).then(res => {\n        console.log(res)\n        this.tableData = res.data.records\n        this.total = res.data.total\n      })\n    },\n\n\n    save(){\n      request.post(\"/user\")\n    },\n\n    handleSizeChange(){\n\n    },\n    handleCurrentChange(){\n\n    },\n    handleEidt(){\n\n    },\n    handleDelete(){\n\n    },\n\n  },\n}\n</script>\n"]},"metadata":{},"sourceType":"module"}