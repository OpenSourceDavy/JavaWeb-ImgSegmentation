{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: \"Login\",\n\n  data() {\n    return {\n      form: {},\n      dialogVisible: false,\n      rulesLogin: {\n        username: [{\n          required: true,\n          message: '请输入用户名',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }],\n        role: [{\n          required: true,\n          message: '请选择用户类别',\n          trigger: 'blur'\n        }]\n      },\n      rulesRegister: {\n        username: [{\n          required: true,\n          message: '请输入用户名',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }],\n        confirm: [{\n          required: true,\n          message: '请再次输入密码',\n          trigger: 'blur'\n        }],\n        nickName: [{\n          required: true,\n          message: '请输入姓名',\n          trigger: 'blur'\n        }],\n        age: [{\n          required: true,\n          message: '请输入年龄',\n          trigger: 'blur'\n        }],\n        sex: [{\n          required: true,\n          message: '请选择性别',\n          trigger: 'blur'\n        }],\n        address: [{\n          required: true,\n          message: '请输入您的住址',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n\n  created() {\n    sessionStorage.removeItem(\"user\");\n  },\n\n  methods: {\n    login() {\n      this.$refs['formLogin'].validate(valid => {\n        if (valid) {\n          request.post(\"/user/login\", this.form).then(res => {\n            console.log(res);\n\n            if (res.code === '0') {\n              this.$message({\n                type: \"success\",\n                message: \"登陆成功\"\n              });\n              sessionStorage.setItem(\"user\", JSON.stringify(res.data));\n              this.$router.push(\"/home\"); //登陆成功后进行页面跳转，跳转到主页\n            } else {\n              this.$message({\n                type: \"error\",\n                message: res.msg\n              });\n            }\n          });\n        }\n      });\n    },\n\n    register() {\n      this.dialogVisible = true;\n      this.form = {};\n    },\n\n    save() {\n      this.$refs['formRegister'].validate(valid => {\n        if (valid) {\n          if (this.form.password != this.form.confirm) {\n            this.$message({\n              type: \"error\",\n              message: \"两次密码输入不一致！\"\n            });\n            return;\n          }\n\n          request.post(\"/user/register\", this.form).then(res => {\n            console.log(res);\n\n            if (res.code === '0') {\n              this.$message({\n                type: \"success\",\n                message: \"注册成功\"\n              });\n              this.$router.push(\"/login\"); //登陆成功后进行页面跳转，跳转到主页\n\n              this.dialogVisible = false;\n            } else {\n              this.$message({\n                type: \"error\",\n                message: res.msg\n              });\n            }\n          });\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgFA,OAAOA,OAAP,MAAoB,iBAApB;AAEA,eAAe;AACbC,MAAI,EAAE,OADO;;AAEbC,MAAI,GAAE;AACJ,WAAO;AACLC,UAAI,EAAC,EADA;AAELC,mBAAa,EAAC,KAFT;AAGLC,gBAAU,EAAE;AACVC,gBAAQ,EAAE,CACR;AAAEC,kBAAQ,EAAE,IAAZ;AAAkBC,iBAAO,EAAE,QAA3B;AAAqCC,iBAAO,EAAE;AAA9C,SADQ,CADA;AAIVC,gBAAQ,EAAE,CACR;AAAEH,kBAAQ,EAAE,IAAZ;AAAkBC,iBAAO,EAAE,OAA3B;AAAoCC,iBAAO,EAAE;AAA7C,SADQ,CAJA;AAOVE,YAAI,EAAE,CACJ;AAAEJ,kBAAQ,EAAE,IAAZ;AAAkBC,iBAAO,EAAE,SAA3B;AAAsCC,iBAAO,EAAE;AAA/C,SADI;AAPI,OAHP;AAcLG,mBAAa,EAAE;AACbN,gBAAQ,EAAE,CACR;AAAEC,kBAAQ,EAAE,IAAZ;AAAkBC,iBAAO,EAAE,QAA3B;AAAqCC,iBAAO,EAAE;AAA9C,SADQ,CADG;AAIbC,gBAAQ,EAAE,CACR;AAAEH,kBAAQ,EAAE,IAAZ;AAAkBC,iBAAO,EAAE,OAA3B;AAAoCC,iBAAO,EAAE;AAA7C,SADQ,CAJG;AAObI,eAAO,EAAE,CACP;AAAEN,kBAAQ,EAAE,IAAZ;AAAkBC,iBAAO,EAAE,SAA3B;AAAsCC,iBAAO,EAAE;AAA/C,SADO,CAPI;AAUbK,gBAAQ,EAAE,CACR;AAAEP,kBAA