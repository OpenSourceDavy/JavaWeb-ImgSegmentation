{"ast":null,"code":"import request from \"@/utils/request\";\nimport MyCanvas from \"./Canvas.vue\";\nexport default {\n  name: \"Img\",\n  components: {\n    MyCanvas\n  },\n\n  data() {\n    return {\n      querySearch: [{\n        value: '已评阅'\n      }, {\n        value: '未评阅'\n      }],\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 1,\n      tableData: [],\n      form: [],\n      dialogVisible: false,\n      user: [],\n      isShow: false,\n      isNotShow: true\n    };\n  },\n\n  created() {\n    let str = sessionStorage.getItem(\"user\") || \"{}\";\n    this.user = JSON.parse(str);\n\n    if (this.user.role === '1') {\n      this.isShow = true;\n      this.isNotShow = false;\n    }\n\n    this.load();\n  },\n\n  methods: {\n    load() {\n      this.tableData = [];\n      request.get(\"/img\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        console.log(\"病人\");\n        console.log(res);\n\n        if (this.user.role === '0') {\n          //管理员\n          this.tableData = res.data.records;\n          this.total = res.data.total;\n        } else if (this.user.role === '1') {\n          //病人\n          for (let i = 0; i < res.data.records.length; i++) {\n            if (res.data.records[i].pid == this.user.id) {\n              this.tableData.push(res.data.records[i]);\n            }\n          }\n\n          this.total = this.tableData.length;\n        } else if (this.user.role === '2') {\n          //专家\n          for (let i = 0; i < res.data.records.length; i++) {\n            if (res.data.records[i].sid == this.user.id) {\n              this.tableData.push(res.data.records[i]);\n            }\n          }\n        }\n      });\n    },\n\n    fileUpload(res) {\n      console.log(res);\n      this.form.img = res.data;\n    },\n\n    save() {\n      if (this.form.id) {\n        //更新\n        console.log(\"更新\");\n        console.log(this.form);\n        request.put(\"/img\", this.form).then(res => {\n          console.log(res);\n\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.message\n            });\n          }\n\n          this.load(); //刷新表格\n\n          this.dialogVisible = false;\n        });\n      } else {\n        //新增\n        console.log(\"新增\");\n        console.log(this.form);\n        request.post(\"/img\", this.form).then(res => {\n          console.log(res);\n\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.message\n            });\n          }\n\n          this.load(); //刷新表格\n\n          this.dialogVisible = false;\n        });\n      }\n    },\n\n    handleSizeChange(val) {\n      //改变每页个数\n      this.pageSize = val;\n      this.load();\n    },\n\n    handleCurrentChange(val) {\n      //改变当前页码\n      this.currentPage = val;\n      this.load();\n    },\n\n    upload() {\n      console.log(this.tableData);\n\n      if (this.tableData != null) {\n        this.form = this.tableData[0];\n        this.form.id = '';\n        this.form.pid = this.user.id;\n        this.form.state = '未平阅';\n        this.form.result = '';\n      } // this.form.id = ''\n      // this.form.pid = this.user.id\n      // this.form.state = '未平阅'\n      // this.form.result = ''\n\n\n      this.dialogVisible = true;\n    },\n\n    handleEidt(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n\n      if (this.user.role === '0') {\n        //管理员\n        this.dialogVisible = true;\n      } else if (this.user.role === '1') {\n        //病人\n        this.dialogVisible = true;\n      } else if (this.user.role === '2') {\n        //专家\n        sessionStorage.setItem(\"img\", JSON.stringify(row));\n        this.$router.push(\"/canvas\");\n      } //this.dialogVisible = true\n      // this.$refs.MyCanvas.init(this.form);\n\n    },\n\n    handleDelete(id) {\n      console.log(id);\n      request.delete(\"/img/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.message\n          });\n        }\n\n        this.load(); //刷新表格\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA6GA,OAAOA,OAAP,MAAoB,iBAApB;AACA,OAAOC,QAAP,MAAqB,cAArB;AAGA,eAAe;AACbC,MAAI,EAAE,KADO;AAEbC,YAAU,EAAE;AACVF;AADU,GAFC;;AAKbG,MAAI,GAAE;AACJ,WAAM;AACJC,iBAAW,EAAC,CACV;AAACC,aAAK,EAAC;AAAP,OADU,EAEV;AAACA,aAAK,EAAC;AAAP,OAFU,CADR;AAKJC,YAAM,EAAC,EALH;AAMJC,iBAAW,EAAC,CANR;AAOJC,cAAQ,EAAC,EAPL;AAQJC,WAAK,EAAC,CARF;AASJC,eAAS,EAAC,EATN;AAUJC,UAAI,EAAC,EAVD;AAWJC,mBAAa,EAAC,KAXV;AAYJC,UAAI,EAAC,EAZD;AAaJC,YAAM,EAAC,KAbH;AAcJC,eAAS,EAAC;AAdN,KAAN;AAgBD,GAtBY;;AAuBbC,SAAO,GAAG;AACR,QAAIC,GAAE,GAAIC,cAAc,CAACC,OAAf,CAAuB,MAAvB,KAAkC,IAA5C;AACA,SAAKN,IAAL,GAAYO,IAAI,CAACC,KAAL,CAAWJ,GAAX,CAAZ;;AACA,QAAG,KAAKJ,IAAL,CAAUS,IAAV,KAAmB,GAAtB,EAA0B;AACxB,WAAKR,MAAL,GAAc,IAAd;AACA,WAAKC,SAAL,GAAiB,KAAjB;AACF;;AACA,SAAKQ,IAAL;AACD,GA/BY;;AAiCbC,SAAO,EAAC;AACND,QAAI,GAAE;AACJ,WAAKb,SAAL,GAAiB,EAAjB;AACAX,aAAO,CAAC0B,GAAR,CAAY,MAAZ,EAAmB;AACjBC,cAAM,EAAC;AACLC,iBAAO,EAAC,KAAKpB,WADR;AAELC,kBAAQ,EAAC,KAAKA,QAFT;AAGLF,gBAAM,EAAC,KAAKA;AAHP;AADU,OAAnB,EAMGsB,IANH,CAMQC,GAAE,IAAK;AACbC,eAAO,CAACC,GAAR,CAAY,IAAZ;AACAD,eAAO,CAACC,GAAR,CAAYF,GAAZ;;AACA,YAAG,KAAKhB,IAAL,CAAUS,IAAV,KAAmB,GAAtB,EAA0B;AACxB;AACA,eAAKZ,SAAL,GAAiBmB,GAAG,CAAC1B,IAAJ,CAAS6B,OAA1B;AACA,eAAKvB,KAAL,GAAaoB,GAAG,CAAC1B,IAAJ,CAASM,KAAtB;AACD,SAJD,MAIM,IAAG,KAAKI,IAAL,CAAUS,IAAV,KAAmB,GAAtB,EAA0B;AAC9B;AAC