{"ast":null,"code":"import request from \"@/utils/request\";\nimport Register from \"./Register.vue\";\nexport default {\n  name: \"Patient\",\n  components: {\n    Register\n  },\n\n  data() {\n    return {\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 1,\n      tableData: [],\n      form: [],\n      dialogVisible: false\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    load() {\n      request.get(\"/user\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search,\n          role: 1\n        }\n      }).then(res => {\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    save() {\n      if (this.form.id) {\n        //更新\n        request.put(\"/user/patient\", this.form).then(res => {\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.message\n            });\n          }\n\n          this.load(); //刷新表格\n\n          this.dialogVisible = false;\n        });\n      }\n    },\n\n    handleSizeChange(val) {\n      //改变每页个数\n      this.pageSize = val;\n      this.load();\n    },\n\n    handleCurrentChange(val) {\n      //改变当前页码\n      this.currentPage = val;\n      this.load();\n    },\n\n    handleEidt(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n\n    handleDelete(id) {\n      request.delete(\"/user/\", {\n        params: {\n          id: id,\n          role: 1\n        }\n      }).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.message\n          });\n        }\n\n        this.load(); //刷新表格\n      });\n    },\n\n    add() {\n      this.$refs.register.init(1);\n      this.form = {};\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAuFA,OAAOA,OAAP,MAAoB,iBAApB;AACA,OAAOC,QAAP,MAAqB,gBAArB;AAEA,eAAe;AACbC,MAAI,EAAE,SADO;AAEbC,YAAU,EAAE;AACVF;AADU,GAFC;;AAKbG,MAAI,GAAE;AACJ,WAAM;AACJC,YAAM,EAAC,EADH;AAEJC,iBAAW,EAAC,CAFR;AAGJC,cAAQ,EAAC,EAHL;AAIJC,WAAK,EAAC,CAJF;AAKJC,eAAS,EAAC,EALN;AAMJC,UAAI,EAAC,EAND;AAOJC,mBAAa,EAAC;AAPV,KAAN;AASD,GAfY;;AAgBbC,SAAO,GAAG;AACR,SAAKC,IAAL;AACD,GAlBY;;AAoBbC,SAAO,EAAC;AACND,QAAI,GAAE;AACJb,aAAO,CAACe,GAAR,CAAY,OAAZ,EAAoB;AAClBC,cAAM,EAAC;AACLC,iBAAO,EAAC,KAAKX,WADR;AAELC,kBAAQ,EAAC,KAAKA,QAFT;AAGLF,gBAAM,EAAC,KAAKA,MAHP;AAILa,cAAI,EAAC;AAJA;AADW,OAApB,EAOGC,IAPH,CAOQC,GAAE,IAAK;AACb,aAAKX,SAAL,GAAiBW,GAAG,CAAChB,IAAJ,CAASiB,OAA1B;AACA,aAAKb,KAAL,GAAaY,GAAG,CAAChB,IAAJ,CAASI,KAAtB;AACD,OAVD;AAWD,KAbK;;AAeNc,QAAI,GAAE;AACJ,UAAG,KAAKZ,IAAL,CAAUa,EAAb,EAAgB;AAAE;AAChBvB,eAAO,CAACwB,GAAR,CAAY,eAAZ,EAA4B,KAAKd,IAAjC,EAAuCS,IAAvC,CAA4CC,GAAE,IAAK;AACjD,cAAGA,GAAG,CAACK,IAAJ,KAAa,GAAhB,EAAoB;AAClB,iBAAKC,QAAL,CAAc;AACZC,kBAAI,EAAC,SADO;AAEZC,qBAAO,EAAC;AAFI,aAAd;AAID,WALD,MAKK;AACH,iBAAKF,QAAL,CAAc;AACZC,kBAAI,EAAC,OADO;AAEZC,qBAAO,EAACR,GAAG,CAACQ;AAFA,aAAd;AAIF;;AACA,eAAKf,IAAL,GAZiD,CAYrC;;AACZ,eAAKF,aAAL,GAAqB,KAArB;AACD,SAdD;AAeF;AACD,KAjCK;;AAmCNkB,oBAAgB,CAACC,GAAD,EAAK;AAAE;AACrB,WAAKvB,QAAL,GAAgBuB,GAAhB;AACA,WAAKjB,IAAL;AACD,KAtCK;;AAuCNkB,uBAAmB,CAACD,GAAD,EAAK;AAAG;AACzB,WAAKxB,WAAL,GAAmBwB,GAAnB;AACA,WAAKjB,IAAL;AACD,KA1CK;;AA2CNmB,cAAU,CAACC,GAAD,EAAK;AACb,WAAKvB,IAAL,GAAYwB,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,SAAL,CAAeH,GAAf,CAAX,CAAZ;AACA,WAAKtB,aAAL,GAAqB,IAArB;AACD,KA9CK;;AA+CN0B,gBAAY,CAACd,EAAD,EAAI;AACdvB,aAAO,CAACsC,MAAR,CAAe,QAAf,EAAwB;AACtBtB,cAAM,EAAC;AACLO,YAAE,EAACA,EADE;AAELL,cAAI,EAAC;AAFA;AADe,OAAxB,EAKGC,IALH,CAKQC,GAAE,IAAI;AACZ,YAAGA,GAAG,CAACK,IAAJ,KAAa,GAAhB,EAAoB;AAClB,eAAKC,QAAL,CAAc;AACZC,gBAAI,EAAC,SADO;AAEZC,mBAAO,EAAC;AAFI,WAAd;AA