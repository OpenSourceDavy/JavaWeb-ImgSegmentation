{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: \"Img\",\n\n  data() {\n    return {\n      querySearch: [{\n        value: '已评阅'\n      }, {\n        value: '未评阅'\n      }],\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 1,\n      tableData: [],\n      form: [],\n      dialogVisible: false\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    load() {\n      request.get(\"/img\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        console.log(\"病人\");\n        console.log(res);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    save() {\n      if (this.form.id) {\n        //更新\n        request.put(\"/img\", this.form).then(res => {\n          console.log(res);\n\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.message\n            });\n          }\n\n          this.load(); //刷新表格\n\n          this.dialogVisible = false;\n        });\n      }\n    },\n\n    handleSizeChange(val) {\n      //改变每页个数\n      this.pageSize = val;\n      this.load();\n    },\n\n    handleCurrentChange(val) {\n      //改变当前页码\n      this.currentPage = val;\n      this.load();\n    },\n\n    handleEidt(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n\n    handleDelete(id) {\n      console.log(id);\n      request.delete(\"/img/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.message\n          });\n        }\n\n        this.load(); //刷新表格\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA6FA,OAAOA,OAAP,MAAoB,iBAApB;AAEA,eAAe;AACbC,MAAI,EAAE,KADO;;AAEbC,MAAI,GAAE;AACJ,WAAM;AACJC,iBAAW,EAAC,CACV;AAACC,aAAK,EAAC;AAAP,OADU,EAEV;AAACA,aAAK,EAAC;AAAP,OAFU,CADR;AAKJC,YAAM,EAAC,EALH;AAMJC,iBAAW,EAAC,CANR;AAOJC,cAAQ,EAAC,EAPL;AAQJC,WAAK,EAAC,CARF;AASJC,eAAS,EAAC,EATN;AAUJC,UAAI,EAAC,EAVD;AAWJC,mBAAa,EAAC;AAXV,KAAN;AAaD,GAhBY;;AAiBbC,SAAO,GAAG;AACR,SAAKC,IAAL;AACD,GAnBY;;AAqBbC,SAAO,EAAC;AACND,QAAI,GAAE;AACJb,aAAO,CAACe,GAAR,CAAY,MAAZ,EAAmB;AACjBC,cAAM,EAAC;AACLC,iBAAO,EAAC,KAAKX,WADR;AAELC,kBAAQ,EAAC,KAAKA,QAFT;AAGLF,gBAAM,EAAC,KAAKA;AAHP;AADU,OAAnB,EAMGa,IANH,CAMQC,GAAE,IAAK;AACbC,eAAO,CAACC,GAAR,CAAY,IAAZ;AACAD,eAAO,CAACC,GAAR,CAAYF,GAAZ;AACA,aAAKV,SAAL,GAAiBU,GAAG,CAACjB,IAAJ,CAASoB,OAA1B;AACA,aAAKd,KAAL,GAAaW,GAAG,CAACjB,IAAJ,CAASM,KAAtB;AACD,OAXD;AAYD,KAdK;;AAgBNe,QAAI,GAAE;AACJ,UAAG,KAAKb,IAAL,CAAUc,EAAb,EAAgB;AAAE;AAChBxB,eAAO,CAACyB,GAAR,CAAY,MAAZ,EAAmB,KAAKf,IAAxB,EAA8BQ,IAA9B,CAAmCC,GAAE,IAAK;AACxCC,iBAAO,CAACC,GAAR,CAAYF,GAAZ;;AACA,cAAGA,GAAG,CAACO,IAAJ,KAAa,GAAhB,EAAoB;AAClB,iBAAKC,QAAL,CAAc;AACZC,kBAAI,EAAC,SADO;AAEZC,qBAAO,EAAC;AAFI,aAAd;AAID,WALD,MAKK;AACH,iBAAKF,QAAL,CAAc;AACZC,kBAAI,EAAC,OADO;AAEZC,qBAAO,EAACV,GAAG,CAACU;AAFA,aAAd;AAIF;;AACA,eAAKhB,IAAL,GAbwC,CAa5B;;AACZ,eAAKF,aAAL,GAAqB,KAArB;AACD,SAfD;AAgBF;AACD,KAnCK;;AAqCNmB,oBAAgB,CAACC,GAAD,EAAK;AAAE;AACrB,WAAKxB,QAAL,GAAgBwB,GAAhB;AACA,WAAKlB,IAAL;AACD,KAxCK;;AAyCNmB,uBAAmB,CAACD,GAAD,EAAK;AAAG;AACzB,WAAKzB,WAAL,GAAmByB,GAAnB;AACA,WAAKlB,IAAL;AACD,KA5CK;;AA6CNoB,cAAU,CAACC,GAAD,EAAK;AACb,WAAKxB,IAAL,GAAYyB,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,SAAL,CAAeH,GAAf,CAAX,CAAZ;AACA,WAAKvB,aAAL,GAAqB,IAArB;AACD,KAhDK;;AAiDN2B,gBAAY,CAACd,EAAD,EAAI;AACdJ,aAAO,CAACC,GAAR,CAAYG,EAAZ;AACAxB,aAAO,CAACuC,MAAR,CAAe,UAAQf,EAAvB,EAA2BN,IAA3B,CAAgCC,GAAE,IAAI;AACpC,YAAGA,GAAG,CAACO,IAAJ,KAAa,GAAhB,EAAoB;AAClB,eAAKC,QAAL,CAAc;AACZC,gBAAI,EAAC,SADO;AAEZC,mBAAO,EAAC;AAFI,WAAd