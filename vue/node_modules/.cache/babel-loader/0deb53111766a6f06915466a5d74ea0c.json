wB,CAIxB;;AACA,aAAKrE,aAAL,GAAqB,IAArB;AACD,OAND,MAMM,IAAG,KAAKI,IAAL,CAAUmB,IAAV,KAAmB,GAAtB,EAA0B;AAC9B,aAAKuD,SAAL,CAAe,MAAK;AAClB,eAAKV,KAAL,CAAW,QAAX,EAAqBC,UAArB,GADkB,CACgB;AACnC,SAFD,EAD8B,CAI9B;;AACA,aAAKrE,aAAL,GAAqB,IAArB;AACD,OANK,MAMA,IAAG,KAAKI,IAAL,CAAUmB,IAAV,KAAmB,GAAtB,EAA0B;AAC9B;AACAJ,sBAAc,CAAC4D,OAAf,CAAuB,KAAvB,EAA6B1D,IAAI,CAACsD,SAAL,CAAehD,GAAf,CAA7B;AACA,aAAKqD,OAAL,CAAaC,IAAb,CAAkB,SAAlB;AACF;AACD,KA3MK;;AA4MNC,gBAAY,CAAClC,EAAD,EAAI;AACd5D,aAAO,CAAC+F,MAAR,CAAe,UAAQnC,EAAvB,EAA2Bd,IAA3B,CAAgCC,GAAE,IAAI;AACpC,YAAGA,GAAG,CAACC,IAAJ,KAAa,GAAhB,EAAoB;AAClB,eAAKC,QAAL,CAAc;AACZC,gBAAI,EAAC,SADO;AAEZC,mBAAO,EAAC;AAFI,WAAd;AAID,SALD,MAKK;AACH,eAAKF,QAAL,CAAc;AACZC,gBAAI,EAAC,OADO;AAEZC,mBAAO,EAACJ,GAAG,CAACI;AAFA,WAAd;AAIF;;AACA,aAAKf,IAAL,GAZoC,CAYxB;AACb,OAbD;AAcD;;AA3NK;AA3DK,CAAf","names":["request","name","components","data","querySearch","value","search","currentPage","pageSize","total","tableData","form","dialogVisible","isShowResult","isShowAi","aiUrl","user","isShow","isNotShow","specialist","patient","result","isS","isP","isA","filesUploadUrl","window","server","time","created","str","sessionStorage","getItem","JSON","parse","role","load","methods","toPython","row","clearTimeout","setTimeout","img","split","put","length","then","res","code","$message","type","message","console","log","openAiUrl","url","features","open","sid","pid","id","get","params","pageNum","records","forEach","td","s","SName","nickName","p","PName","fileUpload","save","post","handleSizeChange","val","handleCurrentChange","upload","state","$refs","clearFiles","changeS","check","downLoad","$alert","confirmButtonText","stringify","flag","handleEidt","$nextTick","setItem","$router","push","handleDelete","delete"],"sourceRoot":"","sources":["C:\\Users\\davy3\\crm_vue1\\vue\\src\\views\\Img.vue"],"sourcesContent":["<template>\r\n  <div style=\"padding: 10px\">\r\n\r\n    <!--    搜索-->\r\n    <div style=\"margin: 10px 0px;\">\r\n      <el-autocomplete\r\n          v-model=\"search\"\r\n          :fetch-suggestions=\"querySearch\"\r\n          placeholder=\"请输入评阅状态\"\r\n          clearable style=\"width:20%;margin-left: 950px\"\r\n      >\r\n        <template #default=\"{ item }\">\r\n          <div class=\"value\">{{ item.value }}</div>\r\n        </template>\r\n      </el-autocomplete>\r\n      <el-button type=\"primary\" style=\"margin-left: 5px;\" @click=\"load\">查询</el-button>\r\n      <el-button type=\"primary\" style=\"margin-left: 5px;\" @click=\"upload\" v-show=\"isShow\">上传</el-button>\r\n    </div>\r\n    <el-table :data=\"tableData\" border style=\"width: 100%\" stripe>\r\n      <el-table-column prop=\"id\" label=\"X光图ID\" sortable/>\r\n      <el-table-column prop=\"PName\" label=\"病人姓名\" />\r\n      <el-table-column prop=\"SName\" label=\"专家姓名\" />\r\n      <el-table-column label=\"X光图\">\r\n        <template #default=\"scope\">\r\n          <el-image\r\n              style=\"width: 100px; height: 100px\"\r\n              :src=\"scope.row.img\"\r\n              :preview-src-list=\"[scope.row.img]\"\r\n              fit=\"cover\"\r\n              preview-teleported=\"true\"\r\n          />\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"专家评阅结果\">\r\n        <template #default=\"scope\">\r\n          <el-button size=\"mini\" @click=\"check(scope.row)\">查看</el-button>\r\n          <el-button size=\"mini\" @click=\"downLoad(scope.row)\">下载</el-button>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column prop=\"state\" label=\"评阅状态\" sortable/>\r\n      <el-table-column prop=\"createTime\" label=\"上传时间\" sortable/>\r\n      <el-table-column label=\"操作\">\r\n        <template #default=\"scope\">\r\n          <el-button size=\"mini\" @click=\"handleEidt(scope.row)\">编辑</el-button>\r\n          <el-popconfirm title=\"你确认删除吗?\" @confirm=\"handleDelete(scope.row.id)\">\r\n            <template #reference>\r\n              <el-button size=\"mini\" type=\"danger\" v-show=\"!isS\">删除</el-button>\r\n            </template>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column label=\"AI识别\">\r\n        <template #default=\"scope\">\r\n          <el-button size=\"mini\" @click=\"toPython(scope.row)\">识别</el-button>\r\n          <el-button size=\"mini\" @click=\"openAiUrl(scope.row)\">查看</el-button>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n\r\n    <div style=\"margin: 10px 0px;float: right\">\r\n      <el-pagination\r\n          v-model:currentPage=\"currentPage\"\r\n          :page-size=\"pageSize\"\r\n          :page-sizes=\"[5, 10, 20]\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\"\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n      />\r\n\r\n      <el-dialog\r\n          v-model=\"dialogVisible\"\r\n          title=\"上传或更新\"\r\n          width=\"30%\"\r\n      >\r\n        <el-form :model=\"form\" label-width=\"120px\">\r\n          <el-form-item label=\"X光图ID\" v-show=\"isNotShow\">\r\n            <el-input v-model=\"form.id\" style=\"width: 160px\" disabled></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"病人ID\" v-show=\"isNotShow\">\r\n            <el-input v-model=\"form.pid\" style=\"width: 160px\" disabled></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"请选择专家\">\r\n            <el-select v-model=\"form.SName\" clearable @change=\"changeS\"\r\n                       size=\"small\">\r\n              <el-option\r\n                  v-for=\"item in specialist\"\r\n                  :key=\"item.id\"\r\n                  :label=\"item.nickName\"\r\n                  :value=\"item.id\">\r\n              </el-option>\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item label=\"专家评阅结果\" v-show=\"isNotShow\">\r\n            <el-input v-model=\"form.result\" style=\"width: 160px\" disabled></el-input>\r\n          </el-form-item>\r\n          <el-form-item label=\"评阅状态\" v-show=\"isNotShow\">\r\n            <el-radio v-model=\"form.state\" label=\"已评阅\">已评阅</el-radio>\r\n            <el-radio v-model=\"form.state\" label=\"未评阅\">未评阅</el-radio>\r\n          </el-form-item>\r\n          <el-form-item label=\"X光图\" style=\"width: 300px\">\r\n            <el-upload\r\n                ref=\"upload\"\r\n                :action = filesUploadUrl\r\n                :on-success=\"fileUpload\"\r\n                style=\"width: 300px\"\r\n            >\r\n              <el-button type=\"primary\">上传</el-button>\r\n            </el-upload>\r\n          </el-form-item>\r\n        </el-form>\r\n        <template #footer>\r\n          <span class=\"dialog-footer\">\r\n            <el-button @click=\"dialogVisible = false\">取消</el-button>\r\n            <el-button type=\"primary\" @click=\"save\">确定</el-button>\r\n      </span>\r\n        </template>\r\n      </el-dialog>\r\n\r\n      <el-dialog\r\n          v-model=\"isShowResult\"\r\n          title=\"专家评阅结果\"\r\n          width=\"50%\"\r\n      >\r\n        <el-input\r\n            type=\"textarea\"\r\n            :rows=\"20\"\r\n            placeholder=\"请输入内容\"\r\n            v-model=\"result\" disabled>\r\n        </el-input>\r\n      </el-dialog>\r\n\r\n      <el-dialog\r\n          v-model=\"isShowAi\"\r\n          title=\"AI识别结果\"\r\n          width=\"50%\"\r\n      >\r\n        <el-image\r\n            style=\"width: 100px; height: 100px\"\r\n            :src=\"aiUrl\"\r\n            :preview-src-list=\"[aiUrl]\"\r\n            fit=\"cover\"\r\n            preview-teleported=\"true\"\r\n        />\r\n      </el-dialog>\r\n\r\n\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport request from \"@/utils/request\";\r\n\r\n\r\nexport default {\r\n  name: \"Img\",\r\n  components: {\r\n  },\r\n  data(){\r\n    return{\r\n      querySearch:[\r\n        {value:'已评阅'},\r\n        {value:'未评阅'},\r\n      ],\r\n      search:'',\r\n      currentPage:1,\r\n      pageSize:10,\r\n      total:1,\r\n      tableData:[],\r\n      form: {},\r\n      dialogVisible:false,\r\n      isShowResult:false,\r\n      isShowAi:false,\r\n      aiUrl:\"\",\r\n      user:{},\r\n      isShow:false,\r\n      isNotShow:true,\r\n      specialist:[],\r\n      patient:[],\r\n      result:'',\r\n      isS:false,\r\n      isP:false,\r\n      isA:false,\r\n      filesUploadUrl:\"http://\" + window.server.filesUploadUrl + \":9090/files/upload\",\r\n      time:'',\r\n    }\r\n  },\r\n  created() {\r\n    let str = sessionStorage.getItem(\"user\") || \"{}\"\r\n    this.user = JSON.parse(str)\r\n    let specialist = sessionStorage.getItem(\"specialist\") || \"{}\"\r\n    this.specialist = JSON.parse(specialist)\r\n    let patient = sessionStorage.getItem(\"patient\") || \"{}\"\r\n    this.patient = JSON.parse(patient)\r\n    if(this.user.role === '0'){\r\n      this.isA = true\r\n      this.isS = false\r\n      this.isP = false\r\n    }else if(this.user.role === '1'){\r\n      this.isShow = true\r\n      this.isNotShow = false\r\n\r\n      this.isA = false\r\n      this.isS = false\r\n      this.isP = true\r\n    }else{\r\n      this.isA = false\r\n      this.isS = true\r\n      this.isP = false\r\n    }\r\n    this.load()\r\n  },\r\n\r\n  methods:{\r\n\r\n\r\n    toPython(row){\r\n      if (this.time) {\r\n        clearTimeout(this.time); //清除\r\n      }\r\n      this.time = setTimeout(()=>{\r\n        let str = row.img.split('/')\r\n        request.put(\"/AI\",str[str.length-1]).then(res => {\r\n          if(res.code === '0'){\r\n            this.$message({\r\n              type:\"success\",\r\n              message:\"更新成功\"\r\n            })\r\n            row.aiUrl = res.data\r\n            console.log(res)\r\n          }else{\r\n            this.$message({\r\n              type:\"error\",\r\n              message:res.message\r\n            })\r\n          }\r\n          this.dialogVisible = false\r\n        })\r\n      },300)\r\n    },\r\n\r\n    openAiUrl(row){\r\n      if(!row.aiUrl){\r\n        this.$message({\r\n          type:\"warning\",\r\n          message:\"图片未识别，请先识别\"\r\n        })\r\n        return false\r\n      }\r\n      this.aiUrl = row.aiUrl\r\n      let str = row.aiUrl.split(\"/\")\r\n      let url = \"http://localhost:9876/file/avatar/\" + str[str.length-1]\r\n      let features = \"height=700,width=1200,top=100,left=200,resizable=yes,scrollings=yes,status=no\";\r\n      window.open(url,\"AI识别结果\",features);\r\n      // this.isShowAi = true\r\n    },\r\n\r\n\r\n    load(){\r\n      this.tableData = []\r\n      let sid = ''\r\n      let pid = ''\r\n      if(this.user.role === '0'){\r\n        //管理员\r\n        sid = ''\r\n        pid = ''\r\n      }else if(this.user.role === '1'){\r\n        //病人\r\n        sid = ''\r\n        pid = this.user.id\r\n      }else if(this.user.role === '2'){\r\n        //专家\r\n        sid = this.user.id\r\n        pid = ''\r\n      }\r\n      request.get(\"/img\",{\r\n        params:{\r\n          pageNum:this.currentPage,\r\n          pageSize:this.pageSize,\r\n          search:this.search,\r\n          sid:sid,\r\n          pid:pid\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data.records\r\n        this.total = res.data.total\r\n        this.tableData.forEach(td=>{\r\n          this.specialist.forEach(s=>{\r\n            if(td.sid == s.id){\r\n              td.SName = s.nickName\r\n            }\r\n          })\r\n          this.patient.forEach(p=>{\r\n            if(td.pid == p.id){\r\n              td.PName = p.nickName\r\n            }\r\n          })\r\n        })\r\n      })\r\n    },\r\n\r\n    fileUpload(res){\r\n      debugger\r\n      this.form.img = res.data\r\n    },\r\n\r\n    save(){\r\n      if(this.form.id){ //更新\r\n        request.put(\"/img\",this.form).then(res => {\r\n          if(res.code === '0'){\r\n            this.$message({\r\n              type:\"success\",\r\n              message:\"更新成功\"\r\n            })\r\n          }else{\r\n            this.$message({\r\n              type:\"error\",\r\n              message:res.message\r\n            })\r\n          }\r\n          this.load() //刷新表格\r\n          this.dialogVisible = false\r\n        })\r\n      }else{ //新增\r\n        request.post(\"/img\",this.form).then(res => {\r\n          if(res.code === '0'){\r\n            this.$message({\r\n              type:\"success\",\r\n              message:\"新增成功\"\r\n            })\r\n          }else{\r\n            this.$message({\r\n              type:\"error\",\r\n              message:res.message\r\n            })\r\n          }\r\n          this.load() //刷新表格\r\n          this.dialogVisible = false\r\n        })\r\n      }\r\n    },\r\n\r\n    handleSizeChange(val){ //改变每页个数\r\n      this.pageSize = val\r\n      this.load()\r\n    },\r\n    handleCurrentChange(val){  //改变当前页码\r\n      this.currentPage = val\r\n      this.load()\r\n    },\r\n    upload(){\r\n\r\n      if(this.tableData!=\"\"){\r\n        this.form = this.tableData[0];\r\n        this.form.id = ''\r\n      }\r\n      this.form.pid = this.user.id\r\n      this.form.state = '未评阅'\r\n      this.form.result = ''\r\n      this.dialogVisible = true\r\n      this.$refs['upload'].clearFiles() //清除历史文件列表\r\n    },\r\n\r\n    changeS(s){\r\n      this.form.sid = s\r\n    },\r\n\r\n    check(row){\r\n      this.isShowResult = true\r\n      this.result = row.result\r\n    },\r\n\r\n    downLoad(row){\r\n      if(row.state == \"未评阅\"){\r\n        this.$alert('该脊椎X光图专家还未评阅，请耐心等待，谢谢', '提示', {\r\n          confirmButtonText: '确定',\r\n          type:\"warning\"\r\n        });\r\n        return false\r\n      }\r\n      if(row.result == \"\"){\r\n        this.$alert('该脊椎X光图专家还未曾给出建议，请耐心等待，谢谢', '提示', {\r\n          confirmButtonText: '确定',\r\n          type:\"warning\"\r\n        });\r\n        return false\r\n      }\r\n      this.form = JSON.parse(JSON.stringify(row))\r\n      let flag = ''\r\n      request.post(\"/files/integration\",this.form).then(res => {\r\n        flag = res.data\r\n        //let url = \"http://175.24.204.121:9090/files/\"+flag  //云服务器配置\r\n        let url = \"http://localhost:9876/api/files/\"+flag\r\n        window.open(url)\r\n      })\r\n    },\r\n\r\n    handleEidt(row){\r\n      this.form = JSON.parse(JSON.stringify(row))\r\n      if(this.user.role === '0'){\r\n        this.$nextTick(() =>{\r\n          this.$refs['upload'].clearFiles() //清除历史文件列表\r\n        })\r\n        //管理员\r\n        this.dialogVisible = true\r\n      }else if(this.user.role === '1'){\r\n        this.$nextTick(() =>{\r\n          this.$refs['upload'].clearFiles() //清除历史文件列表\r\n        })\r\n        //病人\r\n        this.dialogVisible = true\r\n      }else if(this.user.role === '2'){\r\n        //专家\r\n        sessionStorage.setItem(\"img\",JSON.stringify(row))\r\n        this.$router.push(\"/canvas\")\r\n      }\r\n    },\r\n    handleDelete(id){\r\n      request.delete(\"/img/\"+id).then(res =>{\r\n        if(res.code === '0'){\r\n          this.$message({\r\n            type:\"success\",\r\n            message:\"删除成功\"\r\n          })\r\n        }else{\r\n          this.$message({\r\n            type:\"error\",\r\n            message:res.message\r\n          })\r\n        }\r\n        this.load() //刷新表格\r\n      })\r\n    },\r\n\r\n\r\n\r\n\r\n\r\n  },\r\n\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}