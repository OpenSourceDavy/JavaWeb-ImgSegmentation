     this.dialogVisible = true;\n      this.form = {};\n    },\n\n    save() {\n      request.post(\"/user/register\", this.form).then(res => {\n        console.log(res);\n\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"注册成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.message\n          });\n        }\n\n        this.load(); //刷新表格\n\n        this.dialogVisible = false;\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA+DA,OAAOA,OAAP,MAAoB,iBAApB;AAEA,eAAe;AACbC,MAAI,EAAE,OADO;;AAEbC,MAAI,GAAE;AACJ,WAAO;AACLC,UAAI,EAAC,EADA;AAELC,mBAAa,EAAC,KAFT;AAGLC,gBAAU,EAAE;AACVC,gBAAQ,EAAE,CACR;AAAEC,kBAAQ,EAAE,IAAZ;AAAkBC,iBAAO,EAAE,QAA3B;AAAqCC,iBAAO,EAAE;AAA9C,SADQ,CADA;AAIVC,gBAAQ,EAAE,CACR;AAAEH,kBAAQ,EAAE,IAAZ;AAAkBC,iBAAO,EAAE,OAA3B;AAAoCC,iBAAO,EAAE;AAA7C,SADQ;AAJA,OAHP;AAWLE,mBAAa,EAAE;AACbL,gBAAQ,EAAE,CACR;AAAEC,kBAAQ,EAAE,IAAZ;AAAkBC,iBAAO,EAAE,QAA3B;AAAqCC,iBAAO,EAAE;AAA9C,SADQ,CADG;AAIbC,gBAAQ,EAAE,CACR;AAAEH,kBAAQ,EAAE,IAAZ;AAAkBC,iBAAO,EAAE,OAA3B;AAAoCC,iBAAO,EAAE;AAA7C,SADQ,CAJG;AAObG,eAAO,EAAE,CACP;AAAEL,kBAAQ,EAAE,IAAZ;AAAkBC,iBAAO,EAAE,SAA3B;AAAsCC,iBAAO,EAAE;AAA/C,SADO,CAPI;AAUbI,gBAAQ,EAAE,CACR;AAAEN,kBAAQ,EAAE,IAAZ;AAAkBC,iBAAO,EAAE,OAA3B;AAAoCC,iBAAO,EAAE;AAA7C,SADQ,CAVG;AAabK,WAAG,EAAE,CACH;AAAEP,kBAAQ,EAAE,IAAZ;AAAkBQ,aAAG,EAAC,CAAtB;AAAwBC,aAAG,EAAC,GAA5B;AAAgCR,iBAAO,EAAE,cAAzC;AAAyDC,iBAAO,EAAE;AAAlE,SADG,CAbQ;AAgBbQ,WAAG,EAAE,CACH;AAAEV,kBAAQ,EAAE,IAAZ;AAAkBC,iBAAO,EAAE,OAA3B;AAAoCC,iBAAO,EAAE;AAA7C,SADG,CAhBQ;AAmBbS,eAAO,EAAE,CACP;AAAEX,kBAAQ,EAAE,IAAZ;AAAkBC,iBAAO,EAAE,SAA3B;AAAsCC,iBAAO,EAAE;AAA/C,SADO;AAnBI;AAXV,KAAP;AAmCD,GAtCY;;AAuCbU,SAAO,EAAC;AACNC,SAAK,GAAE;AACL,WAAKC,KAAL,CAAW,WAAX,EAAwBC,QAAxB,CAAkCC,KAAD,IAAW;AAC1C,YAAIA,KAAJ,EAAW;AACTvB,iBAAO,CAACwB,IAAR,CAAa,aAAb,EAA2B,KAAKrB,IAAhC,EAAsCsB,IAAtC,CAA2CC,GAAE,IAAK;AAChDC,mBAAO,CAACC,GAAR,CAAYF,GAAZ;;AACA,gBAAIA,GAAG,CAACG,IAAJ,KAAa,G