SystemRole","value","label","gender","methods","init","e","setDialogVisible","role","val","modifyBillDialogCancel","commitData","chinese","pPattern","undefined","$alert","confirmButtonText","type","username","length","password","test","confirm","nickName","reg","sex","age","tel","address","register","f","url","post","then","res","code","$message","message","msg"],"sourceRoot":"","sources":["G:\\springboot+vue\\crm_vue\\vue\\src\\views\\Register.vue"],"sourcesContent":["<template>\r\n  <el-dialog ref=\"dialog\" v-model=\"dialogVisible\" width=\"40%\" >\r\n    <el-container>\r\n      <el-header class=\"el-menu__title\" style=\"margin-bottom:2px;text-align: center\" >\r\n        <span>欢迎注册</span>\r\n      </el-header>\r\n      <el-main >\r\n        <div>\r\n          <el-row>\r\n            <el-col style=\"text-align:center;margin-top:10px\">\r\n              <span>用户类别：</span>\r\n              <el-select v-model=\"user.role\" clearable @change=\"isShow\"\r\n                         size=\"small\">\r\n                <el-option\r\n                    v-for=\"item in SystemRole\"\r\n                    :key=\"item.value\"\r\n                    :label=\"item.label\"\r\n                    :value=\"item.value\">\r\n                </el-option>\r\n              </el-select>\r\n            </el-col>\r\n          </el-row>\r\n          <el-row>\r\n            <el-col style=\"text-align:center;margin-top:10px\">\r\n              <span>&#12288;用户名：</span>\r\n              <el-input v-model=\"user.username\" size=\"small\" style=\"width: 200px;\"></el-input>\r\n            </el-col>\r\n          </el-row>\r\n          <el-row>\r\n            <el-col style=\"text-align:center;margin-top:10px\">\r\n              <span>密&#12288;&#12288;码：</span>\r\n              <el-input v-model=\"user.password\" size=\"small\" style=\"width: 200px;\" show-password></el-input>\r\n            </el-col>\r\n          </el-row>\r\n          <el-row>\r\n            <el-col style=\"text-align:center;margin-top:10px\">\r\n              <span>确认密码：</span>\r\n              <el-input v-model=\"user.confirm\" size=\"small\" style=\"width: 200px;\" show-password></el-input>\r\n            </el-col>\r\n          </el-row>\r\n          <el-row>\r\n            <el-col style=\"text-align:center;margin-top:10px\">\r\n              <span>姓&#12288;&#12288;名：</span>\r\n              <el-input v-model=\"user.nickName\" size=\"small\" style=\"width: 200px;\"></el-input>\r\n            </el-col>\r\n          </el-row>\r\n          <el-row v-show=\"isShowData\">\r\n            <el-col style=\"text-align:center;margin-top:10px\">\r\n              <span>性&#12288;别：</span>\r\n              <el-select v-model=\"user.sex\" clearable\r\n                         size=\"small\">\r\n                <el-option\r\n                    v-for=\"item in gender\"\r\n                    :key=\"item.value\"\r\n                    :label=\"item.label\"\r\n                    :value=\"item.value\">\r\n                </el-option>\r\n              </el-select>\r\n            </el-col>\r\n          </el-row>\r\n          <el-row v-show=\"isShowData\">\r\n            <el-col style=\"text-align:center;margin-top:10px\">\r\n              <span>年&#12288;&#12288;龄：</span>\r\n              <el-input v-model=\"user.age\" size=\"small\" style=\"width: 200px;\"></el-input>\r\n            </el-col>\r\n          </el-row>\r\n          <el-row v-show=\"isShowData\">\r\n            <el-col style=\"text-align:center;margin-top:10px\">\r\n              <span>手机号码：</span>\r\n              <el-input v-model=\"user.tel\" size=\"small\" style=\"width: 200px;\"></el-input>\r\n            </el-col>\r\n          </el-row>\r\n          <el-row v-show=\"isShowData\">\r\n            <el-col style=\"text-align:center;margin-top:10px\">\r\n              <span>家庭住址：</span>\r\n              <el-input v-model=\"user.address\" size=\"small\" style=\"width: 200px;\"></el-input>\r\n            </el-col>\r\n          </el-row>\r\n        </div>\r\n      </el-main>\r\n      <el-footer style=\"text-align: center;\">\r\n        <el-button size=\"mini\" type=\"primary\" @click=\"register()\">确定</el-button>\r\n        <el-button size=\"mini\" type=\"primary\" @click=\"modifyBillDialogCancel()\">取消</el-button>\r\n      </el-footer>\r\n    </el-container>\r\n  </el-dialog>\r\n</template>\r\n\r\n<script>\r\nimport request from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"Register\",\r\n  data(){\r\n    return{\r\n      dialogVisible:false,\r\n      user:{},\r\n      isShowData:true,\r\n      isShow:true,\r\n      SystemRole:[\r\n        {value:\"1\", label:\"病人\"},\r\n        {value:\"2\", label:\"专家\"},\r\n      ],\r\n      gender:[\r\n        {value:\"男\",label:\"男\"},\r\n        {value:\"女\",label:\"女\"},\r\n      ],\r\n    }\r\n  },\r\n\r\n  methods:{\r\n    init(e) {\r\n      this.setDialogVisible(true);\r\n      if(e=='1'){\r\n        this.user.role = '1'\r\n        this.isShow = false\r\n      }else if(e == '2'){\r\n        this.user.role = '2'\r\n        this.isShow = false\r\n      }\r\n    },\r\n    setDialogVisible(val){\r\n      this.dialogVisible = val\r\n      this.user = {}\r\n    },\r\n    modifyBillDialogCancel(){\r\n      this.setDialogVisible(false);\r\n      this.user = {};\r\n    },\r\n    isShow(val){\r\n      if(val === '0'){\r\n        this.isShowData = false;\r\n        this.user= {}\r\n        this.user.role = val\r\n      }else if(val === '1'){\r\n        this.isShowData = true;\r\n        this.user= {}\r\n        this.user.role = val\r\n      }else {\r\n        this.isShowData = true;\r\n        this.user= {}\r\n        this.user.role = val\r\n      }\r\n    },\r\n    commitData() {\r\n      debugger\r\n      let chinese = /^([\\u4e00-\\u9fa5]{2,5})$/;\r\n      //密码强度正则，最少8位，包括至少1个大写字母，1个小写字母，1个数字，1个特殊字符\r\n      let pPattern = /^.*(?=.{8,})(?=.*\\d)(?=.*[A-Z])(?=.*[a-z]).*$/;\r\n      if (this.user.role == \"\" || this.user.role == undefined\r\n          || this.user.role == null) {\r\n        this.$alert('请选择用户类别', '提示', {\r\n          confirmButtonText: '确定',\r\n          type:\"warning\"\r\n        });\r\n        return false;\r\n      } else if (this.user.username == \"\" || this.user.username == undefined\r\n          || this.user.username == null) {\r\n        this.$alert('用户名不能为空', '提示', {\r\n          confirmButtonText: '确定',\r\n          type:\"warning\"\r\n        });\r\n        return false;\r\n      } else if (this.user.username.length <6 || this.user.username.length >20) {\r\n        this.$alert('用户名的长度不能小于6或者大于20', '提示', {\r\n          confirmButtonText: '确定',\r\n          type:\"warning\"\r\n        });\r\n        return false;\r\n      } else if (this.user.password == \"\" || this.user.password == undefined\r\n          || this.user.password == null) {\r\n        this.$alert('密码不能为空', '提示', {\r\n          confirmButtonText: '确定',\r\n          type:\"warning\"\r\n        });\r\n        return false;\r\n      } else if (!pPattern.test(this.user.password)) {\r\n        this.$alert('密码最少8位，包括至少1个大写字母，1个小写字母，1个数字', '提示', {\r\n          confirmButtonText: '确定',\r\n          type:\"warning\"\r\n        });\r\n        return false;\r\n      } else if (this.user.confirm == \"\" || this.user.confirm == undefined\r\n          || this.user.confirm == null) {\r\n        this.$alert('确认密码不能为空', '提示', {\r\n          confirmButtonText: '确定',\r\n          type:\"warning\"\r\n        });\r\n        return false;\r\n      } else if (this.user.password != this.user.confirm) {\r\n        this.$alert('两次密码输入不一致，请重新输入', '提示', {\r\n          confirmButtonText: '确定',\r\n          type:\"warning\"\r\n        });\r\n        return false;\r\n      } else if (this.user.nickName == \"\" || this.user.nickName == undefined\r\n          || this.user.nickName == null) {\r\n        this.$alert('姓名不能为空', '提示', {\r\n          confirmButtonText: '确定',\r\n          type:\"warning\"\r\n        });\r\n        return false;\r\n      } else if (!chinese.test(this.user.nickName)) {\r\n        this.$alert('姓名的长度不能小于2个汉字或者大于5个汉字', '提示', {\r\n          confirmButtonText: '确定',\r\n          type:\"warning\"\r\n        });\r\n        return false;\r\n      }\r\n\r\n      if(this.user.role === '1' || this.user.role === '2' ){\r\n        let reg = /^1[3456789]\\d{9}$/;\r\n        if (this.user.sex == \"\" || this.user.sex == undefined\r\n            || this.user.sex == null) {\r\n          this.$alert('请选择性别', '提示', {\r\n            confirmButtonText: '确定',\r\n            type:\"warning\"\r\n          });\r\n          return false;\r\n        } else if (this.user.age == \"\" || this.user.age == undefined\r\n            || this.user.age == null) {\r\n          this.$alert('请填写您的年龄', '提示', {\r\n            confirmButtonText: '确定',\r\n            type:\"warning\"\r\n          });\r\n          return false;\r\n        } else if (this.user.age < 0 || this.user.age > 120) {\r\n          this.$alert('请填写0到120岁的年龄', '提示', {\r\n            confirmButtonText: '确定',\r\n            type:\"warning\"\r\n          });\r\n          return false;\r\n        } else if (this.user.tel == \"\" || this.user.tel == undefined\r\n            || this.user.tel == null) {\r\n          this.$alert('请填写您的手机号码', '提示', {\r\n            confirmButtonText: '确定',\r\n            type:\"warning\"\r\n          });\r\n          return false;\r\n        } else if (!reg.test(this.user.tel)) {\r\n          this.$alert('请填写正确的手机号码', '提示', {\r\n            confirmButtonText: '确定',\r\n            type:\"warning\"\r\n          });\r\n          return false;\r\n        } else if (this.user.address == \"\" || this.user.address == undefined\r\n            || this.user.address == null) {\r\n          this.$alert('请填写您的家庭住址', '提示', {\r\n            confirmButtonText: '确定',\r\n            type:\"warning\"\r\n          });\r\n          return false;\r\n        }\r\n      }\r\n    },\r\n    register(){\r\n      let f = this.commitData();\r\n      if(f == false){\r\n        return\r\n      }\r\n      let url = \"\"\r\n      if(this.user.role === '0'){\r\n        //管理员登录\r\n        url = \"/root/register\"\r\n      }else if(this.user.role === '1'){\r\n        //病人登录\r\n        url = \"/patient/register\"\r\n      }else{\r\n        //专家登录\r\n        url = \"/specialist/register\"\r\n      }\r\n      request.post(url,this.user).then(res => {\r\n        if (res.code === '0') {\r\n          this.$message({\r\n            type: \"success\",\r\n            message: \"注册成功\"\r\n          })\r\n          this.setDialogVisible(false);\r\n        } else {\r\n          this.$message({\r\n            type: \"error\",\r\n            message: res.msg\r\n          })\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.el-menu__title{\r\n  font-family: \"Microsoft YaHei\";\r\n  font-size: 1.5rem;\r\n  color: #0682C5;\r\n  letter-spacing: 0;\r\n  line-height: 33px;\r\n}\r\n.el-badge-class {\r\n  color: #12A0EB;\r\n  margin-top: 10px;\r\n  margin-left: -30px;\r\n}\r\n.tips-class {\r\n  font-family: PingFangSC-Medium;\r\n  color: #ff0000;\r\n  letter-spacing: 0;\r\n  margin-top: -1.5%;\r\n  font-size: 10px;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}