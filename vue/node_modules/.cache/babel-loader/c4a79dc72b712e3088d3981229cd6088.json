{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: \"Register\",\n\n  data() {\n    return {\n      dialogVisible: false,\n      user: {},\n      isShowData: true,\n      isShow: true,\n      SystemRole: [{\n        value: \"1\",\n        label: \"病人\"\n      }, {\n        value: \"2\",\n        label: \"专家\"\n      }],\n      gender: [{\n        value: \"男\",\n        label: \"男\"\n      }, {\n        value: \"女\",\n        label: \"女\"\n      }]\n    };\n  },\n\n  methods: {\n    init(e) {\n      this.setDialogVisible(true);\n\n      if (e == '1') {\n        this.user.role = '1';\n        this.isShow = false;\n      } else if (e == '2') {\n        this.user.role = '2';\n        this.isShow = false;\n      }\n    },\n\n    setDialogVisible(val) {\n      this.dialogVisible = val;\n      this.user = {};\n    },\n\n    modifyBillDialogCancel() {\n      this.setDialogVisible(false);\n      this.user = {};\n    },\n\n    isShow(val) {\n      if (val === '0') {\n        this.isShowData = false;\n        this.user = {};\n        this.user.role = val;\n      } else if (val === '1') {\n        this.isShowData = true;\n        this.user = {};\n        this.user.role = val;\n      } else {\n        this.isShowData = true;\n        this.user = {};\n        this.user.role = val;\n      }\n    },\n\n    commitData() {\n      debugger;\n      let chinese = /^([\\u4e00-\\u9fa5]{2,5})$/; //密码强度正则，最少8位，包括至少1个大写字母，1个小写字母，1个数字，1个特殊字符\n\n      let pPattern = /^.*(?=.{8,})(?=.*\\d)(?=.*[A-Z])(?=.*[a-z]).*$/;\n\n      if (this.user.role == \"\" || this.user.role == undefined || this.user.role == null) {\n        this.$alert('请选择用户类别', '提示', {\n          confirmButtonText: '确定',\n          type: \"warning\"\n        });\n        return false;\n      } else if (this.user.username == \"\" || this.user.username == undefined || this.user.username == null) {\n        this.$alert('用户名不能为空', '提示', {\n          confirmButtonText: '确定',\n          type: \"warning\"\n        });\n        return false;\n      } else if (this.user.username.length < 6 || this.user.username.length > 20) {\n        this.$alert('用户名的长度不能小于6或者大于20', '提示', {\n          confirmButtonText: '确定',\n          type: \"warning\"\n        });\n        return false;\n      } else if (this.user.password == \"\" || this.user.password == undefined || this.user.password == null) {\n        this.$alert('密码不能为空', '提示', {\n          confirmButtonText: '确定',\n          type: \"warning\"\n        });\n        return false;\n      } else if (!pPattern.test(this.user.password)) {\n        this.$alert('密码最少8位，包括至少1个大写字母，1个小写字母，1个数字', '提示', {\n          confirmButtonText: '确定',\n          type: \"warning\"\n        });\n        return false;\n      } else if (this.user.confirm == \"\" || this.user.confirm == undefined || this.user.confirm == null) {\n        this.$alert('确认密码不能为空', '提示', {\n          confirmButtonText: '确定',\n          type: \"warning\"\n        });\n        return false;\n      } else if (this.user.password != this.user.confirm) {\n        this.$alert('两次密码输入不一致，请重新输入', '提示', {\n          confirmButtonText: '确定',\n          type: \"warning\"\n        });\n        return false;\n      } else if (this.user.nickName == \"\" || this.user.nickName == undefined || this.user.nickName == null) {\n        this.$alert('姓名不能为空', '提示', {\n          confirmButtonText: '确定',\n          type: \"warning\"\n        });\n        return false;\n      } else if (!chinese.test(this.user.nickName)) {\n        this.$alert('姓名的长度不能小于2个汉字或者大于5个汉字', '提示', {\n          confirmButtonText: '确定',\n          type: \"warning\"\n        });\n        return false;\n      }\n\n      if (this.user.role === '1' || this.user.role === '2') {\n        let reg = /^1[3456789]\\d{9}$/;\n\n        if (this.user.sex == \"\" || this.user.sex == undefined || this.user.sex == null) {\n          this.$alert('请选择性别', '提示', {\n            confirmButtonText: '确定',\n            type: \"warning\"\n          });\n          return false;\n        } else if (this.user.age == \"\" || this.user.age == undefined || this.user.age == null) {\n          this.$alert('请填写您的年龄', '提示', {\n            confirmButtonText: '确定',\n            type: \"warning\"\n          });\n          return false;\n        } else if (this.user.age < 0 || this.user.age > 120) {\n          this.$alert('请填写0到120岁的年龄', '提示', {\n            confirmButtonText: '确定',\n            type: \"warning\"\n          });\n          return false;\n        } else if (this.user.tel == \"\" || this.user.tel == undefined || this.user.tel == null) {\n          this.$alert('请填写您的手机号码', '提示', {\n            confirmButtonText: '确定',\n            type: \"warning\"\n          });\n          return false;\n        } else if (!reg.test(this.user.tel)) {\n          this.$alert('请填写正确的手机号码', '提示', {\n            confirmButtonText: '确定',\n            type: \"warning\"\n          });\n          return false;\n        } else if (this.user.address == \"\" || this.user.address == undefined || this.user.address == null) {\n          this.$alert('请填写您的家庭住址', '提示', {\n            confirmButtonText: '确定',\n            type: \"warning\"\n          });\n          return false;\n        }\n      }\n    },\n\n    register() {\n      let f = this.commitData();\n\n      if (f == false) {\n        return;\n      }\n\n      let url = \"\";\n\n      if (this.user.role === '0') {\n        //管理员登录\n        url = \"/root/register\";\n      } else if (this.user.role === '1') {\n        //病人登录\n        url = \"/patient/register\";\n      } else {\n        //专家登录\n        url = \"/specialist/register\";\n      }\n\n      request.post(url, this.user).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"注册成功\"\n          });\n          this.setDialogVisible(false);\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          });\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyFA,OAAOA,OAAP,MAAoB,iBAApB;AAEA,eAAe;AACbC,MAAI,EAAE,UADO;;AAEbC,MAAI,GAAE;AACJ,WAAM;AACJC,mBAAa,EAAC,KADV;AAEJC,UAAI,EAAC,EAFD;AAGJC,gBAAU,EAAC,IAHP;AAIJC,YAAM,EAAC,IAJH;AAKJC,gBAAU,EAAC,CACT;AAACC,aAAK,EAAC,GAAP;AAAYC,aAAK,EAAC;AAAlB,OADS,EAET;AAACD,aAAK,EAAC,GAAP;AAAYC,aAAK,EAAC;AAAlB,OAFS,CALP;AASJC,YAAM,EAAC,CACL;AAACF,aAAK,EAAC,GAAP;AAAWC,aAAK,EAAC;AAAjB,OADK,EAEL;AAACD,aAAK,EAAC,GAAP;AAAWC,aAAK,EAAC;AAAjB,OAFK;AATH,KAAN;AAcD,GAjBY;;AAmBbE,SAAO,EAAC;AACNC,QAAI,CAACC,CAAD,EAAI;AACN,WAAKC,gBAAL,CAAsB,IAAtB;;AACA,UAAGD,CAAC,IAAE,GAAN,EAAU;AACR,aAAKT,IAAL,CAAUW,IAAV,GAAiB,GAAjB;AACA,aAAKT,MAAL,GAAc,KAAd;AACD,OAHD,MAGM,IAAGO,KAAK,GAAR,EAAY;AAChB,aAAKT,IAAL,CAAUW,IAAV,GAAiB,GAAjB;AACA,aAAKT,MAAL,GAAc,KAAd;AACF;AACD,KAVK;;AAWNQ,oBAAgB,CAACE,GAAD,EAAK;AACnB,WAAKb,aAAL,GAAqBa,GAArB;AACA,WAAKZ,IAAL,GAAY,EAAZ;AACD,KAdK;;AAeNa,0BAAsB,GAAE;AACtB,WAAKH,gBAAL,CAAsB,KAAtB;AACA,WAAKV,IAAL,GAAY,EAAZ;AACD,KAlBK;;AAmBNE,UAAM,CAACU,GAAD,EAAK;AACT,UAAGA,GAAE,KAAM,GAAX,EAAe;AACb,aAAKX,UAAL,GAAkB,KAAlB;AACA,aAAKD,IAAL,GAAW,EAAX;AACA,aAAKA,IAAL,CAAUW,IAAV,GAAiBC,GAAjB;AACD,OAJD,MAIM,IAAGA,GAAE,KAAM,GAAX,EAAe;AACnB,aAAKX,UAAL,GAAkB,IAAlB;AACA,aAAKD,IAAL,GAAW,EAAX;AACA,aAAKA,IAAL,CAAUW,IAAV,GAAiBC,GAAjB;AACD,OAJK,MAIA;AACJ,aAAKX,UAAL,GAAkB,IAAlB;AACA,aAAKD,IAAL,GAAW,EAAX;AACA,aAAKA,IAAL,CAAUW,IAAV,GAAiBC,GAAjB;AACF;AACD,KAjCK;;AAkCNE,cAAU,GAAG;AACX;AACA,UAAIC,OAAM,GAAI,0BAAd,CAFW,CAGX;;AACA,UAAIC,QAAO,GAAI,+CAAf;;AACA,UAAI,KAAKhB,IAAL,CAAUW,IAAV,IAAkB,EAAlB,IAAwB,KAAKX,IAAL,CAAUW,IAAV,IAAkBM,SAA1C,IACG,KAAKjB,IAAL,CAAUW,IAAV,IAAkB,IADzB,EAC+B;AAC7B,aAAKO,MAAL,CAAY,SAAZ,EAAuB,IAAvB,EAA6B;AAC3BC,2BAAiB,EAAE,IADQ;AAE3BC,cAAI,EAAC;AAFsB,SAA7B;AAIA,eAAO,KAAP;AACF,OAPA,MAOO,IAAI,KAAKpB,IAAL,CAAUqB,QAAV,IAAsB,EAAtB,IAA4B,KAAKrB,IAAL,CAAUqB,QAAV,IAAsBJ,SAAlD,IACJ,KAAKjB,IAAL,CAAUqB,QAAV,IAAsB,IADtB,EAC4B;AACjC,aAAKH,MAAL,CAAY,SAAZ,EAAuB,IAAvB,EAA6B;AAC3BC,2BAAiB,EAAE,IADQ;AAE3BC,cAAI,EAAC;AAFsB,SAA7B;AAIA,eAAO,KAAP