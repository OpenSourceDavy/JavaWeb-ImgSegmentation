{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: \"Img\",\n  components: {},\n\n  data() {\n    return {\n      querySearch: [{\n        value: '已评阅'\n      }, {\n        value: '未评阅'\n      }],\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 1,\n      tableData: [],\n      form: {},\n      dialogVisible: false,\n      isShowResult: false,\n      user: {},\n      isShow: false,\n      isNotShow: true,\n      specialist: [],\n      patient: [],\n      result: '',\n      isS: false,\n      isP: false,\n      isA: false,\n      filesUploadUrl: \"http://\" + window.server.filesUploadUrl + \":9090/files/upload\",\n      time: ''\n    };\n  },\n\n  created() {\n    let str = sessionStorage.getItem(\"user\") || \"{}\";\n    this.user = JSON.parse(str);\n    let specialist = sessionStorage.getItem(\"specialist\") || \"{}\";\n    this.specialist = JSON.parse(specialist);\n    let patient = sessionStorage.getItem(\"patient\") || \"{}\";\n    this.patient = JSON.parse(patient);\n\n    if (this.user.role === '0') {\n      this.isA = true;\n      this.isS = false;\n      this.isP = false;\n    } else if (this.user.role === '1') {\n      this.isShow = true;\n      this.isNotShow = false;\n      this.isA = false;\n      this.isS = false;\n      this.isP = true;\n    } else {\n      this.isA = false;\n      this.isS = true;\n      this.isP = false;\n    }\n\n    this.load();\n  },\n\n  methods: {\n    toPython(row) {\n      if (this.time) {\n        clearTimeout(this.time); //清除\n      }\n\n      this.time = setTimeout(() => {\n        let str = row.img.split('.');\n\n        if (!str[1]) {\n          row.img += '.jpg';\n        }\n\n        request.put(\"/AI\", row.img).then(res => {\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            });\n            console.log(res);\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.message\n            });\n          }\n\n          this.dialogVisible = false;\n        });\n      }, 300);\n    },\n\n    load() {\n      this.tableData = [];\n      let sid = '';\n      let pid = '';\n\n      if (this.user.role === '0') {\n        //管理员\n        sid = '';\n        pid = '';\n      } else if (this.user.role === '1') {\n        //病人\n        sid = '';\n        pid = this.user.id;\n      } else if (this.user.role === '2') {\n        //专家\n        sid = this.user.id;\n        pid = '';\n      }\n\n      request.get(\"/img\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search,\n          sid: sid,\n          pid: pid\n        }\n      }).then(res => {\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n        this.tableData.forEach(td => {\n          this.specialist.forEach(s => {\n            if (td.sid == s.id) {\n              td.SName = s.nickName;\n            }\n          });\n          this.patient.forEach(p => {\n            if (td.pid == p.id) {\n              td.PName = p.nickName;\n            }\n          });\n        });\n      });\n    },\n\n    fileUpload(res) {\n      this.form.img = res.data;\n    },\n\n    save() {\n      if (this.form.id) {\n        //更新\n        request.put(\"/img\", this.form).then(res => {\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.message\n            });\n          }\n\n          this.load(); //刷新表格\n\n          this.dialogVisible = false;\n        });\n      } else {\n        //新增\n        request.post(\"/img\", this.form).then(res => {\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            });\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.message\n            });\n          }\n\n          this.load(); //刷新表格\n\n          this.dialogVisible = false;\n        });\n      }\n    },\n\n    handleSizeChange(val) {\n      //改变每页个数\n      this.pageSize = val;\n      this.load();\n    },\n\n    handleCurrentChange(val) {\n      //改变当前页码\n      this.currentPage = val;\n      this.load();\n    },\n\n    upload() {\n      if (this.tableData != \"\") {\n        this.form = this.tableData[0];\n        this.form.id = '';\n      }\n\n      this.form.pid = this.user.id;\n      this.form.state = '未评阅';\n      this.form.result = '';\n      this.dialogVisible = true;\n      this.$refs['upload'].clearFiles(); //清除历史文件列表\n    },\n\n    changeS(s) {\n      this.form.sid = s;\n    },\n\n    check(row) {\n      this.isShowResult = true;\n      this.result = row.result;\n    },\n\n    downLoad(row) {\n      if (row.state == \"未评阅\") {\n        this.$alert('该脊椎X光图专家还未评阅，请耐心等待，谢谢', '提示', {\n          confirmButtonText: '确定',\n          type: \"warning\"\n        });\n        return false;\n      }\n\n      if (row.result == \"\") {\n        this.$alert('该脊椎X光图专家还未曾给出建议，请耐心等待，谢谢', '提示', {\n          confirmButtonText: '确定',\n          type: \"warning\"\n        });\n        return false;\n      }\n\n      this.form = JSON.parse(JSON.stringify(row));\n      let flag = '';\n      request.post(\"/files/integration\", this.form).then(res => {\n        flag = res.data; //let url = \"http://175.24.204.121:9090/files/\"+flag  //云服务器配置\n\n        let url = \"http://localhost:9876/api/files/\" + flag;\n        window.open(url);\n      });\n    },\n\n    handleEidt(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n\n      if (this.user.role === '0') {\n        this.$nextTick(() => {\n          this.$refs['upload'].clearFiles(); //清除历史文件列表\n        }); //管理员\n\n        this.dialogVisible = true;\n      } else if (this.user.role === '1') {\n        this.$nextTick(() => {\n          this.$refs['upload'].clearFiles(); //清除历史文件列表\n        }); //病人\n\n        this.dialogVisible = true;\n      } else if (this.user.role === '2') {\n        //专家\n        sessionStorage.setItem(\"img\", JSON.stringify(row));\n        this.$router.push(\"/canvas\");\n      }\n    },\n\n    handleDelete(id) {\n      request.delete(\"/img/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          });\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.message\n          });\n        }\n\n        this.load(); //刷新表格\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA0IA,OAAOA,OAAP,MAAoB,iBAApB;AAGA,eAAe;AACbC,MAAI,EAAE,KADO;AAEbC,YAAU,EAAE,EAFC;;AAIbC,MAAI,GAAE;AACJ,WAAM;AACJC,iBAAW,EAAC,CACV;AAACC,aAAK,EAAC;AAAP,OADU,EAEV;AAACA,aAAK,EAAC;AAAP,OAFU,CADR;AAKJC,YAAM,EAAC,EALH;AAMJC,iBAAW,EAAC,CANR;AAOJC,cAAQ,EAAC,EAPL;AAQJC,WAAK,EAAC,CARF;AASJC,eAAS,EAAC,EATN;AAUJC,UAAI,EAAE,EAVF;AAWJC,mBAAa,EAAC,KAXV;AAYJC,kBAAY,EAAC,KAZT;AAaJC,UAAI,EAAC,EAbD;AAcJC,YAAM,EAAC,KAdH;AAeJC,eAAS,EAAC,IAfN;AAgBJC,gBAAU,EAAC,EAhBP;AAiBJC,aAAO,EAAC,EAjBJ;AAkBJC,YAAM,EAAC,EAlBH;AAmBJC,SAAG,EAAC,KAnBA;AAoBJC,SAAG,EAAC,KApBA;AAqBJC,SAAG,EAAC,KArBA;AAsBJC,oBAAc,EAAC,YAAYC,MAAM,CAACC,MAAP,CAAcF,cAA1B,GAA2C,oBAtBtD;AAuBJG,UAAI,EAAC;AAvBD,KAAN;AAyBD,GA9BY;;AA+BbC,SAAO,GAAG;AACR,QAAIC,GAAE,GAAIC,cAAc,CAACC,OAAf,CAAuB,MAAvB,KAAkC,IAA5C;AACA,SAAKhB,IAAL,GAAYiB,IAAI,CAACC,KAAL,CAAWJ,GAAX,CAAZ;AACA,QAAIX,UAAS,GAAIY,cAAc,CAACC,OAAf,CAAuB,YAAvB,KAAwC,IAAzD;AACA,SAAKb,UAAL,GAAkBc,IAAI,CAACC,KAAL,CAAWf,UAAX,CAAlB;AACA,QAAIC,OAAM,GAAIW,cAAc,CAACC,OAAf,CAAuB,SAAvB,KAAqC,IAAnD;AACA,SAAKZ,OAAL,GAAea,IAAI,CAACC,KAAL,CAAWd,OAAX,CAAf;;AACA,QAAG,KAAKJ,IAAL,CAAUmB,IAAV,KAAmB,GAAtB,EAA0B;AACxB,WAAKX,GAAL,GAAW,IAAX;AACA,WAAKF,GAAL,GAAW,KAAX;AACA,WAAKC,GAAL,GAAW,KAAX;AAC