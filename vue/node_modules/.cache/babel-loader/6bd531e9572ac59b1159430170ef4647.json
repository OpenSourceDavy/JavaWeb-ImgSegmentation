{"ast":null,"code":"import request from \"@/utils/request\";\nexport default {\n  name: 'HomeView',\n\n  data() {\n    return {\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 1,\n      tableData: [],\n      form: [],\n      dialogVisible: false\n    };\n  },\n\n  created() {\n    this.load();\n  },\n\n  methods: {\n    load() {\n      request.get(\"/user\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        console.log(res);\n        this.tableData = res.data.records;\n        this.total = res.data.total;\n      });\n    },\n\n    save() {\n      if (this.form.id) {\n        //更新\n        request.put(\"/user\", this.form).then(res => {\n          console.log(res);\n          this.$message({\n            type: \"success\",\n            message: \"更新成功\"\n          });\n        });\n      } else {\n        //新增\n        request.post(\"/user\", this.form).then(res => {\n          console.log(res);\n          this.$message({\n            type: \"success\",\n            message: \"新增成功\"\n          });\n        });\n      }\n    },\n\n    handleSizeChange() {},\n\n    handleCurrentChange() {},\n\n    handleEidt(row) {\n      this.form = JSON.parse(JSON.stringify(row));\n      this.dialogVisible = true;\n    },\n\n    handleDelete() {}\n\n  }\n};","map":{"version":3,"mappings":"AA6EA,OAAOA,OAAP,MAAoB,iBAApB;AAEA,eAAe;AACbC,MAAI,EAAE,UADO;;AAGbC,MAAI,GAAE;AACJ,WAAM;AACJC,YAAM,EAAC,EADH;AAEJC,iBAAW,EAAC,CAFR;AAGJC,cAAQ,EAAC,EAHL;AAIJC,WAAK,EAAC,CAJF;AAKJC,eAAS,EAAC,EALN;AAMJC,UAAI,EAAC,EAND;AAOJC,mBAAa,EAAC;AAPV,KAAN;AASD,GAbY;;AAcbC,SAAO,GAAG;AACR,SAAKC,IAAL;AACD,GAhBY;;AAiBbC,SAAO,EAAC;AAEND,QAAI,GAAE;AACJX,aAAO,CAACa,GAAR,CAAY,OAAZ,EAAoB;AAClBC,cAAM,EAAC;AACLC,iBAAO,EAAC,KAAKA,OADR;AAELV,kBAAQ,EAAC,KAAKA,QAFT;AAGLF,gBAAM,EAAC,KAAKA;AAHP;AADW,OAApB,EAMGa,IANH,CAMQC,GAAE,IAAK;AACbC,eAAO,CAACC,GAAR,CAAYF,GAAZ;AACA,aAAKV,SAAL,GAAiBU