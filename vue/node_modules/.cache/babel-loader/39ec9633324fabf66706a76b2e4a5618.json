{"ast":null,"code":"import request from \"@/utils/request\";\nimport Register from \"./Register.vue\";\nexport default {\n  name: \"Login\",\n  components: {\n    Register\n  },\n\n  data() {\n    return {\n      specialist: [],\n      patient: [],\n      form: {},\n      dialogVisible: false,\n      rulesLogin: {\n        username: [{\n          required: true,\n          message: '请输入用户名',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }],\n        role: [{\n          required: true,\n          message: '请选择用户类别',\n          trigger: 'blur'\n        }]\n      },\n      rulesRegister: {\n        username: [{\n          required: true,\n          message: '请输入用户名',\n          trigger: 'blur'\n        }],\n        password: [{\n          required: true,\n          message: '请输入密码',\n          trigger: 'blur'\n        }],\n        confirm: [{\n          required: true,\n          message: '请再次输入密码',\n          trigger: 'blur'\n        }],\n        nickName: [{\n          required: true,\n          message: '请输入姓名',\n          trigger: 'blur'\n        }],\n        age: [{\n          required: true,\n          message: '请输入年龄',\n          trigger: 'blur'\n        }],\n        sex: [{\n          required: true,\n          message: '请选择性别',\n          trigger: 'blur'\n        }],\n        address: [{\n          required: true,\n          message: '请输入您的住址',\n          trigger: 'blur'\n        }]\n      }\n    };\n  },\n\n  created() {\n    sessionStorage.removeItem(\"user\");\n    this.get();\n  },\n\n  methods: {\n    login() {\n      this.$refs['formLogin'].validate(valid => {\n        if (valid) {\n          let url = \"\";\n          let path = \"\";\n\n          if (this.form.role === '0') {\n            //管理员登录\n            url = \"/root/login\";\n            path = \"/patient\";\n          } else if (this.form.role === '1') {\n            //病人登录\n            url = \"/patient/login\";\n            path = \"/img\";\n          } else {\n            //专家登录\n            url = \"/specialist/login\";\n            path = \"/img\";\n          }\n\n          console.log(this.form);\n          request.post(url, this.form).then(res => {\n            console.log(res);\n\n            if (res.code === '0') {\n              this.$message({\n                type: \"success\",\n                message: \"登陆成功\"\n              });\n              sessionStorage.setItem(\"user\", JSON.stringify(res.data));\n              this.$router.push(path); //登陆成功后进行页面跳转，跳转到主页\n            } else {\n              this.$message({\n                type: \"error\",\n                message: res.msg\n              });\n            }\n          });\n        }\n      });\n    },\n\n    get() {\n      request.get(\"/specialist/get\", {}).then(res => {\n        console.log(res);\n        this.specialist = res;\n        console.log(this.specialist);\n      });\n      request.get(\"/patient/get\", {}).then(res => {\n        console.log(res);\n        this.patient = res;\n        console.log(this.patient);\n      });\n    },\n\n    register() {\n      //this.dialogVisible = true;\n      this.$refs.register.init();\n      this.form = {};\n    },\n\n    save() {\n      this.$refs['formRegister'].validate(valid => {\n        if (valid) {\n          if (this.form.password != this.form.confirm) {\n            this.$message({\n              type: \"error\",\n              message: \"两次密码输入不一致！\"\n            });\n            return;\n          }\n\n          console.log(\"注册user\");\n          console.log(this.form);\n          request.post(\"/user/register\", this.form).then(res => {\n            console.log(res);\n\n            if (res.code === '0') {\n              this.$message({\n                type: \"success\",\n                message: \"注册成功\"\n              });\n              this.$router.push(\"/login\"); //登陆成功后进行页面跳转，跳转到主页\n\n              this.dialogVisible = false;\n            } else {\n              this.$message({\n                type: \"error\",\n                message: res.msg\n              });\n            }\n          });\n        }\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgFA,OAAOA,OAAP,MAAoB,iBAApB;AACA,OAAOC,QAAP,MAAqB,gBAArB;AAEA,eAAe;AACbC,MAAI,EAAE,OADO;AAEbC,YAAU,EAAE;AACVF;AADU,GAFC;;AAKbG,MAAI,GAAE;AACJ,WAAO;AACLC,gBAAU,EAAC,EADN;AAELC,aAAO,EAAC,EAFH;AAGLC,UAAI,EAAC,EAHA;AAILC,mBAAa,EAAC,KAJT;AAKLC,gBAAU,EAAE;AACVC,gBAAQ,EAAE,CACR;AAAEC,kBAAQ,EAAE,IAAZ;AAAkBC,iBAAO,EAAE,QAA3B;AAAqCC,iBAAO,EAAE;AAA9C,SADQ,CADA;AAIVC,gBAAQ,EAAE,CACR;AAAEH,kBAAQ,EAAE,IAAZ;AAAkBC,iBAAO,EAAE,OAA3B;AAAoCC,iBAAO,EAAE;AAA7C,SADQ,CAJA;AAOVE,YAAI,EAAE,CACJ;AAAEJ,kBAAQ,EAAE,IAAZ;AAAkBC,iBAAO,EAAE,SAA3B;AAAsCC,iBAAO,EAAE;AAA/C,SADI;AAPI,OALP;AAgBLG,mBAAa,EAAE;AACbN,gBAAQ,EAAE,CACR;AAAEC,kBAAQ,EAAE,IAAZ;AAAkBC,iBAAO,EAAE,QAA3B;AAAqCC,iBAAO,EAAE;AAA9C,SADQ,CADG;AAIbC,gBAAQ,EAAE,CACR;AAAEH,kBAAQ,EAAE,IAAZ;AAAkBC,iBAAO,EAAE,OAA3B;AAAoCC,iBAAO,EAAE;AAA7C,SADQ,CAJG;AAObI,eAAO,EAAE,CACP;AAAEN,kBAAQ,EAAE,IAAZ;AAAkBC,iBAAO,EAAE,SAA3B;AAAsCC,iBAAO,EAAE;AAA/C,SADO,CAPI;AAUbK,gBAAQ,EAAE,CACR;AAAEP,kBAAQ,EAAE,IAAZ;AAAkBC,iBAAO,EAAE,OAA3B;AAAoCC,iBAAO,EAAE;AAA7C,SADQ,CAVG;AAabM,WAAG,EAAE,CACH;AAAER,kBAAQ,EAAE,IAAZ;AAAiBC,iBAAO,EAAE,OAA1B;AAAmCC,iBAAO,EAAE;AAA5C,SADG,CAbQ;AAgBbO,WAAG,EAAE,CACH;AAAET,kBAAQ,EAAE,IAAZ;AAAkBC,iBAAO,EAAE,OAA3B;AAAoCC,iBAAO,EAAE;AAA7C,SADG,CAhBQ;AAmBbQ,eAAO,EAAE,CACP;AAAEV,kBAAQ,EAAE,IAAZ;AAAkBC,iBAAO,EAAE,SAA3B;AAAsCC,iBAAO,EAAE;AAA/C,SADO;AAnBI;AAhBV,KAAP;AAwCD,GA9CY;;AA+CbS,SAAO,GAAG;AACRC,kBAAc,CAACC,UAAf,CAA0B,MAA1B;AACA,SAAKC,GAAL;AACD,GAlDY;;AAmDbC,SAAO,EAAC;AACNC,SAAK,GAAE;AACL,WAAKC,KAAL,CAAW,WAAX,EAAwBC,QAAxB,CAAkCC,KAAD,IAAW;AAC1C,YAAIA,KAAJ,EAAW;AACT,cAAIC,GAAE,GAAI,EAAV;AACA,cAAIC,IAAG,GAAI,EAAX;;AACA,cAAG,KAAKzB,IAAL,CAAUQ,IAAV,KAAmB,GAAtB,EAA0B;AACxB;AACAgB,eAAE,GAAI,aAAN;AACAC,gBAAG,GAAI,UAAP;AACD,WAJD,MAIM,IAAG,KAAKzB,IAAL,CAAUQ,IAAV,KAAmB,GAAtB,EAA0B;AAC9B;AACAgB,eAAE,GAAI